{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["analysis_id", "verdict"],
  "properties": {
    "analysis_id": {"type": "string"},
    "verdict": {"type": "string"},
    "summary": {"type": "string"},
    "seed_summaries": {"type": "array", "items": {"type": "object"}},
    "evidence_support_index": {"type": "object"},
    "analysis_artifacts": {"type": "object"},
    "mitre_candidates": {"type": "array"},
    "insights": {"type": "array", "items": {"type": "string"}},
    "driver_guidance": {"type": "array"},
    "execution_guidance": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "case_id": {"type": "string"},
          "primary_seed_id": {"type": "string"},
          "seed_ids": {"type": "array", "items": {"type": "string"}},
          "category_id": {"type": "string"},
          "package_name": {"type": "string"},
          "target_capability": {"type": "string"},
          "environment_capabilities": {"type": "object"},
          "prerequisites": {"type": "array"},
          "steps": {"type": "array"},
          "success_criteria": {"type": "array"},
          "cleanup": {"type": "array"}
        }
      }
    }
  }
}
